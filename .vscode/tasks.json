{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "wasm-pack - build",
			"type": "shell",
			"command": "wasm-pack build --target web",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$rustc"
			]
		},
		{
			"label": "deno - http-server",
			"type": "shell",
			"command": "file_server .",
			"group": "none",
			"problemMatcher": "$tsc",
		}
	]
}